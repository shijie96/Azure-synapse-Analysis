{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory199662"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"AzureSynapseAnalytics1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics1'"
		},
		"datalakelinkage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakelinkage'"
		},
		"datapoollink_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'datapoollink'"
		},
		"datalakelinkage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakegen21996.dfs.core.windows.net/"
		},
		"datapoollink_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "synapseworkspace96.sql.azuresynapse.net"
		},
		"datapoollink_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "synapsededicatedpool"
		},
		"datapoollink_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory199662_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AzureSynapseAnalytics1_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics1_connectionString')]"
					},
					"datalakelinkage_accountKey": {
						"value": "[parameters('datalakelinkage_accountKey')]"
					},
					"datapoollink_password": {
						"value": "[parameters('datapoollink_password')]"
					},
					"datalakelinkage_properties_typeProperties_url": {
						"value": "[parameters('datalakelinkage_properties_typeProperties_url')]"
					},
					"datapoollink_properties_typeProperties_server": {
						"value": "[parameters('datapoollink_properties_typeProperties_server')]"
					},
					"datapoollink_properties_typeProperties_database": {
						"value": "[parameters('datapoollink_properties_typeProperties_database')]"
					},
					"datapoollink_properties_typeProperties_userName": {
						"value": "[parameters('datapoollink_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}