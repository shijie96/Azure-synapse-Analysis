			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "appdbserver96_SalesOrderHeader",
								"type": "DatasetReference"
							},
							"name": "SaleOrderHeaderStream"
						},
						{
